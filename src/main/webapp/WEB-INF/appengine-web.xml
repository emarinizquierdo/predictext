<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

  <application>predictext</application>
  <version>1-0-0</version>

  <!-- Allows App Engine to send multiple requests to one instance in parallel -->
  <threadsafe>true</threadsafe>

  <!-- Configure java.util.logging -->
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    <property name="user.home" value=".sheets.googleapis.com-java-quickstart" />
  </system-properties>

  <!-- HTTP Sessions are disabled by default. To enable HTTP sessions specify: -->
  <sessions-enabled>true</sessions-enabled>

  <!-- It's possible to reduce request latency by configuring your application
      to asynchronously write HTTP session data to the datastore -->
  <async-session-persistence enabled="false" />

  <public-root>/app/</public-root>

</appengine-web-app>
